{
    "name": "gzhegow/di",
    "description": "Dependency-Injection Container",
    "type": "library",
    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8",
        
        "symfony/var-dumper": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Gzhegow\\Di\\": "src/gzhegow/Di"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "config/symfony.var-dumper.php",
            
            "helpers/dd.php"
        ]
    }
}
