  "require": {
    "php": ">=7.3",
    
    "psr/container": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    
    "symfony/var-dumper": "^5.3"
  },
  
  "autoload": {
    "psr-4": {
      "Gzhegow\\Di\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gzhegow\\Di\\Tests\\": "tests/src"
    }
  }
}
